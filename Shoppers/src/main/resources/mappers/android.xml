<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="android">
<insert id="insert-reply-android">
insert into and-reply(p_no, writer, cont)
values(#{p_no}, #{writer}, #{cont}) 
</insert>

<select id="select-by-p_no" resultType="com.online.shop.domain.AndroidVO">
select * from and-reply where p_no = #{p_no}
</select>
</mapper>